          "affected_components": [
            {
              "code": "pjmpxvq2cmr2",
              "name": "Copilot",
              "old_status": "partial_outage",
              "new_status": "partial_outage"
            }
          ],
          "deliver_notifications": true,
          "custom_tweet": null,
          "tweet_id": null
        },
        {
          "id": "3m4dpxvyc7hd",
          "status": "investigating",
          "body": "We are continuing to investigate and test solutions internally while working with our model provider on a deeper investigation into the cause. We will update again when we have identified a mitigation.",
          "incident_id": "l94jr9wnhs4r",
          "created_at": "2025-10-01T18:16:57.321Z",
          "updated_at": "2025-10-01T18:16:57.321Z",
