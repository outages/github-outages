        },
        {
          "id": "fkzbszfb46c7",
          "status": "investigating",
          "body": "We are testing other internal mitigations so that we can return to the higher maximum input length. We are still working with our upstream model provider to understand the contributing factors for this sudden decrease in input limits.",
          "incident_id": "l94jr9wnhs4r",
          "created_at": "2025-10-01T17:37:55.766Z",
          "updated_at": "2025-10-01T17:37:55.766Z",
